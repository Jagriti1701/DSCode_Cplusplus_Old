//It wiil not work if numbers are equal
#include <stdio.h>
int main()
{
  int x = 6, y = 7;
 
  // Code to swap 'x' (0110) and 'y' (0111)
  x = x ^ y;  // x now becomes 1 (0001)
  y = x ^ y;  // y becomes 6 (0110)
  x = x ^ y;  // x becomes 7 (0111)
 
  printf("After Swapping: x = %d, y = %d", x, y);
 
  return 0;
}
